{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "redtt",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.redtt",
				"match": "\\b(V|in|with|where|begin|end|let|dim|elim|fst|snd|coe|com|pair|comp|fun|hcom|vproj|vin|lam|refl|pre|kan|type|data|opaque|def|meta|normalize|print|public|private|import|quit)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.redtt",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.redtt",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.redtt"
}